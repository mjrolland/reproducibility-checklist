	journal = {Environmental Health Perspectives},
	author = {Thayer, Kristina A. and Taylor, Kyla W. and Garantziotis, Stavros and Schurman, Shepherd H. and Kissling, Grace E. and Hunt, Dawn and Herbert, Brenda and Church, Rebecca and Jankowich, Rachael and Churchwell, Mona I. and Scheri, Richard C. and Birnbaum, Linda S. and Bucher, John R.},
	month = apr,
	year = {2016},
