	pages = {567--574},
	file = {Le Moal et al. - 2014 - Semen quality trends in French regions are consist.pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\ZDV9XQEJ\\Le Moal et al. - 2014 - Semen quality trends in French regions are consist.pdf:application/pdf},
