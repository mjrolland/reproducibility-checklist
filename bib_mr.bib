	pmid = {21406337},
	pmcid = {PMC3223007},
	keywords = {Female, Benzhydryl Compounds, Biomarkers, Humans, Chromatography, High Pressure Liquid, Solid Phase Extraction, Male, Adult, Middle Aged, Environmental Exposure, Chromatography, Gas, Circadian Rhythm, Environmental Monitoring, Environmental Pollutants, Georgia, Mass Spectrometry, Phenols, Time Factors},
	pages = {983--988},
