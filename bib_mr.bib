	number = {1},
	urldate = {2021-10-18},
	journal = {Pediatric Research},
	author = {Kaul, Ylva Fredriksson and Rosander, Kerstin and von Hofsten, Claes and Brodd, Katarina Strand and Holmström, Gerd and Kaul, Alexander and Böhm, Birgitta and Hellström-Westas, Lena},
