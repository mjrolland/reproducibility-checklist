@article{asta_modifying_2019,
	title = {The {Modifying} {Role} of {Socioeconomic} {Position} and {Greenness} on the {Short}-{Term} {Effect} of {Heat} and {Air} {Pollution} on {Preterm} {Births} in {Rome}, 2001–2013},
