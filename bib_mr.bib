	author = {Jovanovic, Nicolas and Mustieles, Vicente and Althuser, Marc and Lyon-Caen, Sarah and Alfaidy, Nadia and Thomsen, Cathrine and Sakhi, Amrit Kaur and Sabaredzovic, Azemira and Bayat, Sam and Couturier-Tarrade, Anne and Slama, Rémy and Philippat, Claire},
	year = {2024},
	pmid = {38689737},
