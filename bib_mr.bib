	journal = {Archives of Disease in Childhood - Fetal and Neonatal Edition},
	author = {Linder, Nehama and Hiersch, Liran and Fridman, Elana and Klinger, Gil and Lubin, Daniel and Kouadio, Franck and Melamed, Nir},
	month = jul,
	year = {2017},
	pages = {F286--F290},
