	month = apr,
	year = {2022},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {SARS-CoV-2, Epidemiology, Viral infection, Data mining},
	pages = {1957},
	file = {Full Text PDF:C\:\\Users\\rollmatt\\Zotero\\storage\\BDJT353J\\Whitaker et al. - 2022 - Persistent COVID-19 symptoms in a community study .pdf:application/pdf},
