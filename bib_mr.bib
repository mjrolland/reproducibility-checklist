	urldate = {2023-02-21},
	journal = {Environmental Health Perspectives},
	author = {Silva, Manori J and Barr, Dana B and Reidy, John A and Malek, Nicole A and Hodge, Carolyn C and Caudill, Samuel P and Brock, John W and Needham, Larry L and Calafat, Antonia M},
	month = mar,
