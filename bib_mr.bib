	number = {4},
	urldate = {2024-02-05},
	journal = {International Journal of Environmental Research and Public Health},
	author = {Azzolina, Danila and Lorenzoni, Giulia and Bressan, Silvia and Da Dalt, Liviana and Baldi, Ileana and Gregori, Dario},
