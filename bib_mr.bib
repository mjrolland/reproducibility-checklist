	number = {12},
	urldate = {2022-05-09},
	journal = {PLoS ONE},
	author = {Factor-Litvak, Pam and Insel, Beverly and Calafat, Antonia M. and Liu, Xinhua and Perera, Frederica and Rauh, Virginia A. and Whyatt, Robin M.},
