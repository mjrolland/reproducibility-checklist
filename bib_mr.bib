@article{miodovnik_endocrine_2011,
	title = {Endocrine disruptors and childhood social impairment},
	volume = {32},
	issn = {0161813X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0161813X10002354},
	doi = {10.1016/j.neuro.2010.12.009},
	abstract = {Prenatal exposure to endocrine disruptors has the potential to impact early brain development. Neurodevelopmental toxicity in utero may manifest as psychosocial deﬁcits later in childhood. This study investigates prenatal exposure to two ubiquitous endocrine disruptors, the phthalate esters and bisphenol A (BPA), and social behavior in a sample of adolescent inner-city children. Third trimester urines of women enrolled in the Mount Sinai Children’s Environmental Health Study between 1998 and 2002 (n = 404) were analyzed for phthalate metabolites and BPA. Mother–child pairs were asked to return for a follow-up assessment when the child was between the ages of 7 and 9 years. At this visit, mothers completed the Social Responsiveness Scale (SRS) (n = 137), a quantitative scale for measuring the severity of social impairment related to Autistic Spectrum Disorders (ASD) in the general population. In adjusted general linear models increasing log-transformed low molecular weight (LMW) phthalate metabolite concentrations were associated with greater social deﬁcits (b = 1.53, 95\% CI 0.25–2.8). Among the subscales, LMWP were also associated with poorer Social Cognition (b = 1.40, 95\% CI 0.1–2.7); Social Communication (b = 1.86, 95\% CI 0.5–3.2); and Social Awareness (b = 1.25, 95\% CI 0.1–2.4), but not for Autistic Mannerisms or Social Motivation. No signiﬁcant association with BPA was found (b = 1.18, 95\% CI À0.75, 3.11). Prenatal phthalate exposure was associated with childhood social impairment in a multiethnic urban population. Even mild degrees of impaired social functioning in otherwise healthy individuals can have very important adverse effects over a child’s lifetime. These results extend our previous ﬁnding of atypical neonatal and early childhood behaviors in relation to prenatal phthalate exposure.},
	language = {en},
	number = {2},
	urldate = {2022-03-14},
	journal = {NeuroToxicology},
	author = {Miodovnik, Amir and Engel, Stephanie M. and Zhu, Chenbo and Ye, Xiaoyun and Soorya, Latha V. and Silva, Manori J. and Calafat, Antonia M. and Wolff, Mary S.},
	month = mar,
	year = {2011},
	pages = {261--267},
	file = {Miodovnik et al. - 2011 - Endocrine disruptors and childhood social impairme.pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\4ZZ2XFDY\\Miodovnik et al. - 2011 - Endocrine disruptors and childhood social impairme.pdf:application/pdf},
}

