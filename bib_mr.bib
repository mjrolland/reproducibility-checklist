	pmcid = {PMC9537670},
	pages = {157--168},
	file = {PubMed Central Full Text PDF:C\:\\Users\\rollmatt\\Zotero\\storage\\GLPWV3B2\\Prosperi et Chiarelli - 2022 - COVID-19 and diabetes in children.pdf:application/pdf},
}

