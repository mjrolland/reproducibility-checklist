	keywords = {Female, Pregnancy, Benzhydryl Compounds, Humans, Male, Cognition, Child, Phenols, BPA, Prenatal Exposure Delayed Effects, Cognitive function, Prenatal exposure, Bisphenols, BPF, BPS, Sweden},
	pages = {106433},
	file = {Texte intégral:C\:\\Users\\rollmatt\\Zotero\\storage\\EZPF2IEV\\Bornehag et al. - 2021 - Prenatal exposure to bisphenols and cognitive func.pdf:application/pdf},
