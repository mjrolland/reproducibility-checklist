	author = {Poimenova, A. and Markaki, E. and Rahiotis, C. and Kitraki, E.},
	month = may,
	year = {2010},
	pmid = {20219646},
	keywords = {Animals, Dose-Response Relationship, Drug, Female, Maze Learning, Pregnancy, Rats, Rats, Wistar, Endocrine Disruptors, Benzhydryl Compounds, Male, Environmental Exposure, Phenols, Estrogens, Non-Steroidal, Prenatal Exposure Delayed Effects, Brain, Hypothalamo-Hypophyseal System, Pituitary-Adrenal System, Anxiety, Corticosterone, Hippocampus, Sex Characteristics, Adaptation, Psychological, Exploratory Behavior, Memory Disorders, Neurosecretory Systems, Receptors, Steroid},
