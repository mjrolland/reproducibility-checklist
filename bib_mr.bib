	file = {Casas et al. - 2016 - Exposure to Bisphenol A and Phthalates during Preg.pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\MV3YM5JJ\\Casas et al. - 2016 - Exposure to Bisphenol A and Phthalates during Preg.pdf:application/pdf},
