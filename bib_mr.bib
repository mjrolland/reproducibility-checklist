	month = feb,
	year = {2020},
	pages = {027004},
	file = {Oulhote et al. - 2020 - Gestational Exposures to Phthalates and Folic Acid.pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\P3SX62CS\\Oulhote et al. - 2020 - Gestational Exposures to Phthalates and Folic Acid.pdf:application/pdf},
