	journal = {JAMA Network Open},
	author = {Fernández-de-las-Peñas, César and Rodríguez-Jiménez, Jorge and Cancela-Cilleruelo, Ignacio and Guerrero-Peral, Angel and Martín-Guerrero, José D. and García-Azorín, David and Cornejo-Mazzuchelli, Ana and Hernández-Barrera, Valentín and Pellicer-Valero, Oscar J.},
	month = nov,
	year = {2022},
	pages = {e2242106},
	file = {Snapshot:C\:\\Users\\rollmatt\\Zotero\\storage\\R3TBHXRH\\2798446.html:text/html;Texte intégral:C\:\\Users\\rollmatt\\Zotero\\storage\\5WA4C84Z\\Fernández-de-las-Peñas et al. - 2022 - Post–COVID-19 Symptoms 2 Years After SARS-CoV-2 In.pdf:application/pdf},
}

