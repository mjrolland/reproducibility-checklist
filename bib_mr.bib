	author = {Lassen, Tina Harmer and Frederiksen, Hanne and Kyhl, Henriette Boye and Swan, Shanna H. and Main, Katharina M. and Andersson, Anna-Maria and Lind, Dorte Vesterholm and Husby, Steffen and Wohlfahrt-Veje, Christine and Skakkebæk, Niels E. and Jensen, Tina Kold},
