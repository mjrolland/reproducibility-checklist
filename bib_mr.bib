	author = {Fernández-de-Las-Peñas, César and Martín-Guerrero, José D. and Florencio, Lidiane L. and Navarro-Pardo, Esperanza and Rodríguez-Jiménez, Jorge and Torres-Macho, Juan and Pellicer-Valero, Oscar J.},
