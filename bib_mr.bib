	keywords = {Female, Pregnancy, Endocrine Disruptors, Humans, Child, Preschool, Infant, Male, Infant, Newborn, Child, Environmental Pollutants, Phenols, Bayes Theorem, Cytokines, Endocrine disruptors, Hierarchical BKMR, Interleukin-6, Internalizing behaviour, Mediation, Mixture, Mother-child birth cohort, Neurodevelopment, Parabens, Prenatal Exposure Delayed Effects, Tumor Necrosis Factor-alpha},
	pages = {115865},
	file = {Version soumise:C\:\\Users\\rollmatt\\Zotero\\storage\\57HPZ4L3\\Khalfallah et al. - 2023 - Cytokines as mediators of the associations of pren.pdf:application/pdf},
