	file = {Whitcomb et al. - 2010 - Treatment of Batch in the Detection, Calibration, .pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\5E3ZLQX3\\Whitcomb et al. - 2010 - Treatment of Batch in the Detection, Calibration, .pdf:application/pdf},
