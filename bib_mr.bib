	pages = {990--1002},
	file = {Ashrap et al. - 2018 - Elevated concentrations of urinary triclocarban, p.pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\L68KUMV9\\Ashrap et al. - 2018 - Elevated concentrations of urinary triclocarban, p.pdf:application/pdf},
