@article{clark_should_2015,
	title = {Should {I} {Use} {Fixed} or {Random} {Effects}?},
	volume = {3},
	issn = {2049-8470, 2049-8489},
	url = {http://www.journals.cambridge.org/abstract_S2049847014000326},
	doi = {10.1017/psrm.2014.32},
	abstract = {Empirical analyses in political science very often confront data that are clustered or grouped. To account for group-level variation, and improve model ﬁt, researchers will commonly specify either a ﬁxed or random eﬀects model. But current advice on which approach should be preferred, and under what conditions, remains vague and sometimes contradictory. We cast the problem from an experimental, rather than theoretical, perspective, and oﬀer a typology of dataset characteristics that can help guide applied researchers in their model choice. Through a series of Monte Carlo simulations, we evaluate the total error due to bias and variance in the inferences of each model, for many diﬀerent sizes and types of datasets common in political science research. Our analysis further demonstrates that the popular Hausman speciﬁcation test is nether a necessary nor suﬃcient statistic for choosing between ﬁxed and random eﬀects.},
