	file = {Day et al. - 2021 - Phthalate mixtures in pregnancy, autistic traits, .pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\BHEAI6GE\\Day et al. - 2021 - Phthalate mixtures in pregnancy, autistic traits, .pdf:application/pdf},
