	keywords = {Biomarkers, Humans, Environmental Exposure, Environmental Monitoring, Environmental Pollutants},
	pages = {A166--168},
	file = {Texte intégral:C\:\\Users\\rollmatt\\Zotero\\storage\\7VC45SQH\\Calafat et al. - 2015 - Optimal Exposure Biomarkers for Nonpersistent Chem.pdf:application/pdf},
