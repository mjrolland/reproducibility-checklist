	pmcid = {PMC4961013},
	keywords = {Female, Humans, Male, Adult, Visual Perception, Adolescent, Young Adult, Time Factors, Auditory Perception, Brain Mapping, Cerebral Cortex, declarative memory, functional connectivity, Hippocampus, intersubject correlation, Magnetic Resonance Imaging, Memory, Episodic, Memory, Long-Term, Memory, Short-Term, naturalistic, Neural Pathways, Neuropsychological Tests, timescales, Video Recording},
	pages = {3428--3441},
	file = {Texte intégral:C\:\\Users\\rollmatt\\Zotero\\storage\\YZSZCPPG\\Chen et al. - 2016 - Accessing Real-Life Episodic Information from Minu.pdf:application/pdf},
