	journal = {Environment International},
	author = {Ashrap, Pahriya and Watkins, Deborah J. and Calafat, Antonia M. and Ye, Xiaoyun and Rosario, Zaira and Brown, Phil and Vélez-Vega, Carmen M. and Alshawabkeh, Akram and Cordero, José F. and Meeker, John D.},
	month = dec,
