	author = {Jankowska, Agnieszka and Polańska, Kinga and Hanke, Wojciech and Wesołowska, Ewelina and Ligocka, Danuta and Waszkowska, Małgorzata and Stańczak, Aleksander and Tartaglione, Anna Maria and Mirabella, Fiorino and Chiarotti, Flavia and Garí, Mercè and Calamandrei, Gemma},
