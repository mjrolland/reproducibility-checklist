	journal = {Epidemiology},
	author = {Jedynak, Paulina and Rolland, Matthieu and Pin, Isabelle and Thomsen, Cathrine and Sakhi, Amrit K. and Sabaredzovic, Azemira and Philippat, Claire and Slama, Rémy and {the SEPAGES Study Group}},
