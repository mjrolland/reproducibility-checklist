@article{vernet_withinday_2018,
	title = {Within-{Day}, {Between}-{Day}, and {Between}-{Week} {Variability} of {Urinary} {Concentrations} of {Phenol} {Biomarkers} in {Pregnant} {Women}},
	volume = {126},
	issn = {0091-6765, 1552-9924},
	url = {https://ehp.niehs.nih.gov/doi/10.1289/EHP1994},
	doi = {10.1289/EHP1994},
	abstract = {BACKGROUND: Toxicology studies have shown adverse eﬀects of developmental exposure to industrial phenols. Evaluation in humans is challenged by potentially marked within-subject variability of phenol biomarkers in pregnant women, which is poorly characterized.
OBJECTIVES: We aimed to characterize within-day, between-day, and between-week variability of phenol urinary biomarker concentrations during pregnancy.
METHODS: In eight French pregnant women, we collected all urine voids over a 1-wk period (average, 60 samples per week per woman) at three occasions (15 ± 2, 24 ± 2, and 32 ± 1 gestational weeks) in 2012–2013. Aliquots of each day and of the whole week were pooled within-subject. We assayed concentrations of 10 phenols in these pools, and, for two women, in all spot (unpooled) samples collected during a 1-wk period. We characterized variability using intraclass correlation coeﬃcients (ICCs) with spot samples (within-day variability), daily pools (between-day variability), and weekly pools (between-week variability).
RESULTS: For most biomarkers, the within-day variability was high (ICCs between 0.03 and 0.50). The between-day variability, based on samples pooled within each day, was much lower, with ICCs {\textgreater}0:60 except for bisphenol S (0.14, 95\% conﬁdence interval [CI]: 0.00, 0.39). The betweenweek variability diﬀered between compounds, with triclosan and bisphenol S having the lowest ICCs ({\textless}0:3) and 2,5-dichlorophenol the highest (ICC {\textgreater}0:9).
CONCLUSION: During pregnancy, phenol biomarkers showed a strong within-day variability, while the variability between days of a given week was more limited. One biospecimen is not enough to eﬃciently characterize exposure; collecting biospecimens during a single week may be enough to represent well the whole pregnancy exposure for some but not all phenols. https://doi.org/10.1289/EHP1994},
	language = {en},
	number = {3},
	urldate = {2022-03-16},
	journal = {Environmental Health Perspectives},
	author = {Vernet, Céline and Philippat, Claire and Calafat, Antonia M. and Ye, Xiaoyun and Lyon-Caen, Sarah and Siroux, Valérie and Schisterman, Enrique F. and Slama, Rémy},
	month = mar,
	year = {2018},
	pages = {037005},
	file = {Vernet et al. - 2018 - Within-Day, Between-Day, and Between-Week Variabil.pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\HXTSRMU8\\Vernet et al. - 2018 - Within-Day, Between-Day, and Between-Week Variabil.pdf:application/pdf},
}

@article{lorber_pharmacokinetic_2002,
	title = {A pharmacokinetic model for estimating exposure of {Americans} to dioxin-like compounds in the past, present, and future},
	volume = {288},
	issn = {00489697},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0048969701011196},
	doi = {10.1016/S0048-9697(01)01119-6},
	abstract = {Empirical evidence suggests that exposure of Americans to dioxin-like compounds was low during the early decades of the 20th century, then increased during the 1940s and 1950s, reaching a peak in the 1960s and 1970s, and progressively decreased to lower levels in the 1980s and 1990s. Such evidence includes dioxin analysis of carbon-dated sediment cores of lakes and rivers, preserved meat samples from different decades of the 20th century, and limited body burden measurements of dioxin-like compounds. Pinsky and Lorber (1998) summarized studies measuring 2,3,7,8-TCDD in blood and adipose tissue, and found a range of 10–20 pgyg (ppt) lipid during the 1970s, and 2–10 ppt lipid during the 1980s. This study reviews body burdens of dioxin toxic equivalents, TEQs, to find a range from approximately 50 to 80 ppt lipid during the 1970s, 30–50 ppt lipid during the 1980s, and 10–20 ppt lipid during the 1990s (TEQs comprised of the 17 dioxin and furan congeners only). ,Pinsky and Lorber (1998) investigated historical exposure trends for 2,3,7,8-TCDD by using a single-compartment, first-order pharmacokinetic model. The current study extends this prior effort by modeling dioxin TEQs instead of the single compound, 2,3,7,8-TCDD. TEQs are modeled as though they were a single compound, in contrast to an approach where the individual dioxin and furan congeners are modeled separately. It was found that body burdens of TEQs during the 1970s, 1980s and 1990s could be modeled by assuming a historical dose which began the century at low levels of approximately 0.5 pg TEQykgyday, rose during the middle decades of the 20th century to over 6 pg TEQykgyday, and declined to current levels of approximately 0.5 pg TEQykgyday. Trends in individual and population body burdens of TEQs are also investigated using this PK modeling framework. A key uncertainty of this effort — assuming that TEQs behave as though they were a single compound — is discussed and analyzed. ᮊ 2002 Elsevier Science B.V. All rights reserved.},
	language = {en},
	number = {1-2},
	urldate = {2022-03-16},
	journal = {Science of The Total Environment},
	author = {Lorber, Matthew},
	month = apr,
	year = {2002},
	pages = {81--95},
	file = {Lorber - 2002 - A pharmacokinetic model for estimating exposure of.pdf:C\:\\Users\\rollmatt\\Zotero\\storage\\LS4ZQDY6\\Lorber - 2002 - A pharmacokinetic model for estimating exposure of.pdf:application/pdf},
}

