	keywords = {Humans, Child, Preschool, Adult, Child, Adolescent, Risk Factors, COVID-19, SARS-CoV-2, Hospitalization, coronavirus, cross-immunity, infectious diseases, Intensive Care Units, Patient Acuity, public health},
