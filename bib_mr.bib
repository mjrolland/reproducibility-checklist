	pmcid = {PMC7405326},
	keywords = {Female, Humans, Male, Risk Factors, COVID-19, Acquired Immunodeficiency Syndrome, AIDS, Coinfection, coronavirus disease 2019, Disease Progression, HIV, Medical Informatics Applications, MEDLINE, severe acute respiratory syndrome coronavirus},
