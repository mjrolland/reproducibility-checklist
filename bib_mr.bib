	journal = {Environment International},
	author = {Haug, Line Småstuen and Sakhi, Amrit Kaur and Cequier, Enrique and Casas, Maribel and Maitre, Léa and Basagana, Xavier and Andrusaityte, Sandra and Chalkiadaki, Georgia and Chatzi, Leda and Coen, Muireann and de Bont, Jeroen and Dedele, Audrius and Ferrand, Joane and Grazuleviciene, Regina and Gonzalez, Juan Ramon and Gutzkow, Kristine Bjerve and Keun, Hector and McEachan, Rosie and Meltzer, Helle Margrete and Petraviciene, Inga and Robinson, Oliver and Saulnier, Pierre-Jean and Slama, Rémy and Sunyer, Jordi and Urquiza, José and Vafeiadi, Marina and Wright, John and Vrijheid, Martine and Thomsen, Cathrine},
	month = dec,
