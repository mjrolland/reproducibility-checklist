	keywords = {Female, Pregnancy, Humans, Infant, Newborn, Temperature, Prospective Studies, cold, Cold Temperature, exposure windows, heat, Hot Temperature, Premature Birth, Preterm birth},
	pages = {761--773},
	file = {Version soumise:C\:\\Users\\rollmatt\\Zotero\\storage\\YKX7VEF2\\Hough et al. - 2023 - Early delivery following chronic and acute ambient.pdf:application/pdf},
