	pmid = {24487211},
	pmcid = {PMC4230499},
	pages = {282--291},
	file = {PubMed Central Full Text PDF:C\:\\Users\\rollmatt\\Zotero\\storage\\5VYSFK6B\\Tchetgen et VanderWeele - 2014 - On identification of natural direct effects when a.pdf:application/pdf},
}

@article{vansteelandt_natural_2012,
