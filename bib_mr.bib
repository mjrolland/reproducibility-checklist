@article{cirillo_exposure_2015,
	title = {Exposure to {Di}-2-{Ethylhexyl} {Phthalate}, {Di}- \textit{{N}} -{Butyl} {Phthalate} and {Bisphenol} {A} through {Infant} {Formulas}},
	volume = {63},
